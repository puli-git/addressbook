Purpose: To build front end java project
=========================
*/

pipeline{
    agent {
        label 'master'
    }

    stages {
        stage('validate Project'){
               steps{
                   script{
                       mvn validate
                   }
               } 
        }
        stage('Code Compile'){
               steps{
                   script{
                       mvn Compile
                   }
               } 
        }
    
    }
}



